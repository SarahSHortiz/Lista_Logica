#include <stdio.h>

int main() {
    int num, dentro = 0, fora = 0, i = 0;

    printf("Digite o valor ");
    scanf("%d", &num);
    
    do {
        if (num > 10 && num < 157) {
        	            printf("%d estão dentro", i);
            dentro++;
        } else {
        	            printf("%d estão fora\n", i);
            fora++;
        }
        i++;
    
    } while (i < 10);
            printf("%d estão dentro\n %d estão fora\n", dentro, fora);
    return 0;
}



#include <stdio.h>

int main(){
    
int num = 10;

 printf ("%d", num);
    do{  
        if( num % 11 == 0){
    printf("\n ");
              break;
    }
        else{
    printf("\n");
              break;
        }
    num++;
    }
        while ( num <= 100 );
return 0;
}



#include <stdio.h>

int main() {
    int num, dentro = 0, fora = 0, i = 10;

    printf("Digite o valor ");
    scanf("%d", &num);
    
    do {
        if (num % 2 == 0) {
        	            printf("quantidade de par");
            par+= num;
        } else {
        	            printf("quantidade da impar ");
            impar+= impar;
        }
        i++;
        if (par > impar){
        printf("tem mais par %d", par );
        }
        else{
            printf("tem mais impar %d", impar );
        }
    } while (i <= 15);
            printf("%d estão dentro\n %d estão fora\n", dentro, fora);
    return 0;
}


